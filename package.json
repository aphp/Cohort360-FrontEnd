    "build:docker:local": "docker build -t arkhn/cohort-front:latest --build-arg ENV_FILE=.env .",
    "build:docker:staging": "docker build -t arkhn/cohort-front:staging --build-arg ENV_FILE=.env.staging ."
  },
  "browserslist": {
    "production": [
      ">0.2%",
